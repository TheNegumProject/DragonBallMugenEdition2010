; Win state decider
[Statedef 180]
type = S

[State 180, WinVSTien]
type = ChangeState
triggerall = (p2name = "Goten88's Tien")
trigger1 = Time = 0
value = 185 

[State 180, 1]
type = ChangeState
triggerall = (p2name != "Goten88's Tien") && (Life >= 750)
trigger1 = Time = 0
value = Ifelse(random > 500, 181, 186)

[State 180, 2]
type = ChangeState
triggerall = (p2name != "Goten88's Tien") && (Life >= 500 && Life < 750)
trigger1 = Time = 0
value = 182

[State 180, 3]
type = ChangeState
triggerall = (p2name != "Goten88's Tien") && (Life >= 250 && Life < 500)
trigger1 = Time = 0 
value = 183

[State 180, 4]
type = ChangeState
triggerall = (p2name != "Goten88's Tien") && (Life < 250)
trigger1 = Time = 0
value = 184

